{
  "name": "@hass-blocks/homeassistant-typescript",
  "version": "0.3.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/public.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/public.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@swc/helpers": "~0.5.11",
    "dotenv": "16.4.7",
    "execa": "^9.5.2",
    "type-fest": "4.40.0",
    "ws": "8.18.1"
  },
  "devDependencies": {
    "@hass-blocks/local-hass": "workspace:*",
    "@types/ws": "8.18.1",
    "docker-compose": "0.24.3",
    "file-tree-walker-ts": "2.0.3",
    "msw": "2.0.1",
    "package-json-type": "^1.0.3",
    "typescript-docs-verifier": "2.5.3"
  }
}